{
    "name": "pia/pia-skeleton",
    "type": "project",
    "description": "PIA application (skeleton)",
    "keywords": [
        "PIA"
    ],
    "license": "proprietary",
    "authors": [
        {
            "name": "Eddie Palmans",
            "email": "eddie@palmans.nl"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://satis.pia-zorg.nl"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.2",
        "joshbrw/laravel-module-installer": "^2.0",
        "laravel/framework": "^10.48",
        "laravel/tinker": "^2.8",
        "nwidart/laravel-modules": "^10.0",
        "matthewbdaly/laravel-azure-storage": "^2.0",
        "pia/core-module": "~0.1",
        "pia/monthreports-module": "~0.1",
        "pia/demo-module": "~0.1",
        "spatie/laravel-backup": "^8.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "friendsofphp/php-cs-fixer": "^3.6",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.5",
        "spatie/laravel-ignition": "^2.0",
        "spatie/laravel-ray": "^1.32"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Modules\\": "Modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "joshbrw/laravel-module-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
